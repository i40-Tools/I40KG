sto:domainName rdf:type owl:DatatypeProperty ;
               rdfs:domain sto:Domain ;
               rdfs:range rdf:PlainLiteral ;
               rdfs:comment "Points the Standard with the name of the Industrial Domain"@en ;
               rdfs:label "has Domain Name"@en .


