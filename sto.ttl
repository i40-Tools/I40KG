  rdfs:label "OpenFog Consortium" ;
  rdfs:label "OpenFog Consortium"@en ;
  sto:hasOfficialResource <https://www.openfogconsortium.org/> ;
  sto:seeAlso <https://www.openfogconsortium.org/> ;
