#################################################################
#    Object Properties
#################################################################


###  http://purl.org/dc/terms/language
dcterms:language rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith
dul:associatedWith rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isComponentOf
dul:isComponentOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sto:relatedTo ;
                  rdfs:domain sto:Standard ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( sto:Standard
                                             sto:StandardPart
                                           )
                             ] ;
                  rdfs:comment "Represents Standards that are component of other Standards. This relation is not transitive"@en ;
                  rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
                  rdfs:label "is Component Of"@en .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf sto:relatedTo ;
                    rdfs:comment "Standard A has influences in Standard B. For example, IEC_61499 hasInfluenceIn IEC_61804-2(EDDL)"@en ;
                    rdfs:isDefinedBy "https://www.w3.org/TR/prov-o/#wasGeneratedBy"@en ;
                    rdfs:label "was Generated By"@en .


###  http://xmlns.com/foaf/0.1/maker
foaf:maker rdf:type owl:ObjectProperty .


###  https://w3id.org/i40/hasTargetConcern
<https://w3id.org/i40/hasTargetConcern> rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf dul:associatedWith ;
                                        rdfs:domain sto:Standard ;
                                        rdfs:range sto:Concern ;
                                        rdfs:comment "The relation declaring that a standard is relevant or targets a certain concern."@en ;
                                        rdfs:label "has Target Concern"@en .


###  https://w3id.org/i40/sto#alignesWith
sto:alignesWith rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf dul:associatedWith ;
                rdf:type owl:SymmetricProperty ;
                rdfs:domain sto:StandardClassification ;
                rdfs:range sto:StandardClassification ;
                rdfs:comment "indicates that some Standard Classification entity has a similar scope as another. This property most probably is symmetric but not transitive."@en ;
                rdfs:label "alignes With"@en .


###  https://w3id.org/i40/sto#belongsTo
sto:belongsTo rdf:type owl:ObjectProperty ;
              rdfs:domain sto:StandardOrganization ;
              rdfs:range dbo:Country ;
              dcterms:creator sto:irlan ;
              rdfs:comment "A Standard Organization that belongs to the country"@en ;
              rdfs:label "belongs To"@en .


###  https://w3id.org/i40/sto#frames
sto:frames rdf:type owl:ObjectProperty ;
		   rdfs:subPropertyOf dul:associatedWith ;
           rdfs:domain sto:Viewpoint ;
           rdfs:range sto:Concern ;
           rdfs:label "frames"@en ;
           sto:hasOfficialResource <http://www.iso-architecture.org/42010/> ;
           rdfs:comment "A relation specifying that a vewpoint regards a Concern."@en .




###  https://w3id.org/i40/sto#hasAvailableLanguage
sto:hasAvailableLanguage rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf dcterms:language ;
                         rdfs:domain foaf:Document ;
                         rdfs:range dcterms:LinguisticSystem ;
                         dcterms:creator sto:irlan ;
                         rdfs:comment "Languages in which the document of the Standard is available, e.g., English, French, Spanish"@en ,
                                      "Points to Languages in which the standard document is available, e.g., English, French, etc. Defined as subProperty of dcterms:language"@en ;
                         rdfs:label "has Available Language"@en ,
                                    "hasAvailableLanguage"@en .


###  https://w3id.org/i40/sto#hasClassification
sto:hasClassification rdf:type owl:ObjectProperty ;
                      rdfs:domain sto:Standard ;
                      rdfs:range sto:StandardClassification ;
                      dcterms:creator sto:irlan ;
                      rdfs:comment "An Standard is isCategorizedBy a given Categorization Area which is connected to a specific Standarization Initiative"@en ;
                      rdfs:label "has Classification"@en .


sto:isRelevantFor rdf:type owl:ObjectProperty ;
                      rdfs:domain sto:Standard ;
                      rdfs:range sto:RelevantSubject ;
                      rdfs:comment "An Standard is concerned with Relevant Subjects"@en ;
                      rdfs:label "is Relevant for"@en .



###  https://w3id.org/i40/sto#hasDeveloper
sto:hasDeveloper rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf foaf:maker ;
                 rdfs:domain sto:Publication ;
                 rdfs:range sto:StandardOrganization ;
                 dcterms:creator sto:irlan ;
                 rdfs:comment "The organization that developed this Standard"@en ;
                 rdfs:label "has Developer"@en .


###  https://w3id.org/i40/sto#hasDomain
sto:hasDomain rdf:type owl:ObjectProperty ;
              rdfs:domain sto:Publication ;
              rdfs:range sto:Domain .


###  https://w3id.org/i40/sto#hasFileSize
sto:hasFileSize rdf:type owl:ObjectProperty ;
                rdfs:domain foaf:Document ;
                rdfs:range om2:Measure ;
                rdfs:comment "The size of document file in some unit, e.g., 104 Kilobyte."@en ;
                rdfs:label "has File Size"@en ;
                rdfs:seeAlso <https://webstore.iec.ch/webstore/webstore.nsf/xpFAQ.xsp?Open&id=ADMN-7NALM5> ;
                skos:note """1. Reference for om2:Measure <http://www.ontology-of-units-of-measure.org/page/om-2/Measure>
                         2. om2:Measure om2:hasNumericalValue xsd:float;
                         3. om2:Measure om2:hasUnit om2:Unit;
                         4. om2:hasUnit om2:InformationCapacityUnit; is another possibility for range that could not be verified.
                         5. rdfs:seeAlso <http://www.ontology-of-units-of-measure.org/page/om-2/kilobyte> 
                         6. Documentation on Measure <https://github.com/HajoRijgersberg/OM>""" .


###  https://w3id.org/i40/sto#hasHeadquaterIn
sto:hasHeadquaterIn rdf:type owl:ObjectProperty ;
                    rdfs:domain sto:StandardOrganization ;
                    rdfs:range dbo:Place ;
                    dcterms:creator sto:irlan ;
                    rdfs:comment "Denotes the countries in which the Headquarter of the organization who publishes the Standart"@en ;
                    rdfs:label "has Headquater In"@en .


###  https://w3id.org/i40/sto#hasICS
sto:hasICS rdf:type owl:ObjectProperty ;
           rdfs:domain sto:Publication ;
           rdfs:range sto:InternationalClassificationForStandards ;
           rdfs:comment "Serves as a catalogue of International Classification For Standards"@en ;
           rdfs:label "has ICS"@en .


###  https://w3id.org/i40/sto#hasMotivation
sto:hasMotivation rdf:type owl:ObjectProperty ;
                  rdfs:domain sto:Publication ;
                  rdfs:range deo:Motivation .


###  https://w3id.org/i40/sto#hasPreviousVersion
sto:hasPreviousVersion rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf sto:relatedTo ;
                       rdfs:comment "Indicates the list of Standards that are previous versions of a Standard. For example,IEC 60038:2009 has IEC 60038:1983+AMD1:1994+AMD2:1997,IEC 60038:1983/AMD2:1997,IEC 60038:1983/AMD1:1994,IEC 60038:1983,IEC 60038:1975/AMD1:1977,IEC 60038:1975,IEC 60038:1967,IEC 60038:1954,IEC 60038:1938,IEC 60038:1927 as previous versions."@en ;
                       rdfs:label "has Previous Version"@en .


###  https://w3id.org/i40/sto#hasPublisher
sto:hasPublisher rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf foaf:maker ;
                 rdfs:domain sto:Publication ;
                 rdfs:range sto:StandardOrganization ;
                 dcterms:creator sto:irlan ;
                 rdfs:comment "The organization that published this Standard"@en ;
                 rdfs:label "has Publisher"@en .


###  https://w3id.org/i40/sto#hasTechnicalCommittee
sto:hasTechnicalCommittee rdf:type owl:ObjectProperty ;
                          rdfs:domain sto:Publication ;
                          rdfs:range sto:TechnicalCommittee ;
                          rdfs:comment "Specifies the Technical Committee or a Sub-Committee that is responsible for creation and maintenance of a standard"@en ;
                          rdfs:label "has Technical Committee"@en .


###  https://w3id.org/i40/sto#isDescribedin
sto:isDescribedin rdf:type owl:ObjectProperty ;
                  rdfs:domain sto:StandardClassification ;
                  rdfs:range sto:StandardizationFramework ;
                  rdfs:comment "Links the Standarization Frameworks with the Classification of Standards"@en ;
                  rdfs:label "is Described in"@en .


###  https://w3id.org/i40/sto#isImplementationOf
sto:isImplementationOf rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf sto:relatedTo ;
                       dcterms:creator sto:irlan ;
                       rdfs:comment "Represent the relation that connects two standards when standard A is the implementation of standard B, e.g., B2MML isImplementationOf ISA-95."@en ;
                       rdfs:label "is Implementation Of"@en .


###  https://w3id.org/i40/sto#relatedTo
sto:relatedTo rdf:type owl:ObjectProperty ,
                       owl:SymmetricProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain sto:Standard ;
              rdfs:range sto:Standard ;
              dcterms:creator sto:irlan ;
              rdfs:comment "Represent any type of relation that connects two standards."@en ;
              rdfs:label "related To"@en ;
              skos:altLabel "has Associated Standard"@en .


###  https://w3id.org/i40/sto#useStructureOf
sto:useStructureOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf sto:relatedTo ;
                   dcterms:creator sto:irlan ;
                   rdfs:comment "Standard A uses the structure of Standard B. For example, OPC UA uses Structure Of IEC_61804 (EDDL)"@en ;
                   rdfs:label "use Structure Of"@en .


###  https://w3id.org/i40/sto#uses
sto:uses rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf sto:relatedTo ;
         dcterms:creator sto:irlan ;
         rdfs:comment "Defines that Standard A uses Standard B. For example, IEC_62714(AML) uses MathML."@en ;
         rdfs:label "uses"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/i40/sto#abbreviation
sto:abbreviation rdf:type owl:DatatypeProperty ;
                 rdfs:domain owl:Thing ;
                 rdfs:range rdf:PlainLiteral ;
                 rdfs:comment "Abbreviated  name of the thing."@en ;
                 rdfs:label "has Abbreviated Name"@en .


###  https://w3id.org/i40/sto#domainName
sto:domainName rdf:type owl:DatatypeProperty ;
               rdfs:domain sto:Domain ;
               rdfs:range rdf:PlainLiteral ;
               rdfs:comment "Points the Standard with the name of the Industrial Domain"@en ;
               rdfs:label "has Domain Name"@en .


###  https://w3id.org/i40/sto#formationDate
sto:formationDate rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain sto:StandardOrganization ;
                  rdfs:range xsd:date ;
                  rdfs:comment "Formation date of the Organization"@en ;
                  rdfs:label "has Formation Date"@en .


###  https://w3id.org/i40/sto#hasDBpediaResource
sto:hasDBpediaResource rdf:type owl:DatatypeProperty .


###  https://w3id.org/i40/sto#hasEdition
sto:hasEdition rdf:type owl:DatatypeProperty ;
               rdfs:domain foaf:Document ;
               rdfs:range xsd:float ;
               rdfs:comment "Indicates the current Edition/Version number of the document."@en ;
               rdfs:label "has Edition"@en .


###  https://w3id.org/i40/sto#hasImplementationFormat
sto:hasImplementationFormat rdf:type owl:DatatypeProperty ;
