###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:ObjectProperty ;
                owl:equivalentProperty foaf:maker ;
                dcterms:description "Examples of a Creator include a person, an organization, or a service."@en ;
                dcterms:hasVersion <http://dublincore.org/usage/terms/history/#creatorT-002> ;
                dcterms:issued "2008-01-14"^^xsd:date ;
                dcterms:modified "2010-10-11"^^xsd:date ;
                rdfs:comment "An entity primarily responsible for making the resource."@en ;
                rdfs:isDefinedBy dcterms: ;
                rdfs:label "Creator"@en .


###  http://purl.org/dc/terms/language
dcterms:language rdf:type owl:ObjectProperty .


###  http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit
om2:hasUnit rdf:type owl:ObjectProperty ;
            rdfs:range om2:Unit ;
            rdfs:label "has unit"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith
dul:associatedWith rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isComponentOf
dul:isComponentOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :relatedTo ;
                  rdfs:domain :Standard ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( :Standard
                                             :StandardPart
                                           )
                             ] ;
                  rdfs:comment "Represents Standards that are component of other Standards. This relation is not transitive"@en ;
                  rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
                  rdfs:label "is Component Of"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isMemberOf
dul:isMemberOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf dul:associatedWith ;
               rdfs:domain dul:Entity ;
               rdfs:range dul:Collection ;
               rdfs:comment "A relation between collections and entities, e.g. 'the Night Watch by Rembrandt is in the Rijksmuseum collection'; 'Davide is member of the Pen Club', 'Igor is one the subjects chosen for the experiment'."@en ;
               rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
               rdfs:label "is member of"@en ,
                          "Ã¨ membro di"@it .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf
dul:isPartOf rdf:type owl:ObjectProperty ;
             rdfs:label "isPartOf"@en .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :relatedTo ;
                    rdfs:comment "Standard A has influences in Standard B. For example, IEC_61499 hasInfluenceIn IEC_61804-2(EDDL)"@en ;
                    rdfs:isDefinedBy "https://www.w3.org/TR/prov-o/#wasGeneratedBy"@en ;
                    rdfs:label "was Generated By"@en .


###  http://xmlns.com/foaf/0.1/maker
foaf:maker rdf:type owl:ObjectProperty .


###  http://xmlns.com/foaf/0.1/mbox
foaf:mbox rdf:type owl:ObjectProperty ;
          rdfs:domain foaf:Agent .


###  https://w3id.org/i40/sto/alignesWith
:alignesWith rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf dul:associatedWith ;
             rdf:type owl:SymmetricProperty ;
             rdfs:domain :StandardClassification ;
             rdfs:range :StandardClassification ;
             rdfs:comment "indicates that some Standard Classification entity has a similar scope as another. This property most probably is symmetric but not transitive."@en ;
             rdfs:label "alignes With"@en .


###  https://w3id.org/i40/sto/belongsTo
:belongsTo rdf:type owl:ObjectProperty ;
           rdfs:domain :StandardOrganization ;
           rdfs:range dbo:Country .


###  https://w3id.org/i40/sto/frames
:frames rdf:type owl:ObjectProperty ;
        rdfs:domain :StandardClassification ;
        rdfs:range :Concern ;
        rdfs:comment "A relation specifying that a vewpoint regards a Concern."@en ;
        rdfs:label "frames"@en ;
        :hasOfficialResource <http://www.iso-architecture.org/42010/> .


###  https://w3id.org/i40/sto/hasAvailableLanguage
:hasAvailableLanguage rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf dcterms:language ;
                      rdfs:domain foaf:Document ;
                      rdfs:range dcterms:LinguisticSystem .


###  https://w3id.org/i40/sto/hasClassification
:hasClassification rdf:type owl:ObjectProperty ;
                   rdfs:domain :Publication ;
                   rdfs:range :StandardClassification .


###  https://w3id.org/i40/sto/hasDeveloper
:hasDeveloper rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf foaf:maker ;
              rdfs:domain :Publication ;
              rdfs:range :StandardOrganization .


###  https://w3id.org/i40/sto/hasFileSize
:hasFileSize rdf:type owl:ObjectProperty ;
             rdfs:domain foaf:Document ;
             rdfs:range om2:Measure ;
             rdfs:comment "The size of document file in some unit, e.g., 104 Kilobyte."@en ;
             rdfs:label "has File Size"@en ;
             rdfs:seeAlso <https://webstore.iec.ch/webstore/webstore.nsf/xpFAQ.xsp?Open&id=ADMN-7NALM5> ;
             skos:note """1. Reference for om2:Measure <http://www.ontology-of-units-of-measure.org/page/om-2/Measure>
                         2. om2:Measure om2:hasNumericalValue xsd:float;
                         3. om2:Measure om2:hasUnit om2:Unit;
                         4. om2:hasUnit om2:InformationCapacityUnit; is another possibility for range that could not be verified.
                         5. rdfs:seeAlso <http://www.ontology-of-units-of-measure.org/page/om-2/kilobyte> 
                         6. Documentation on Measure <https://github.com/HajoRijgersberg/OM>""" .


###  https://w3id.org/i40/sto/hasHeadquaterIn
:hasHeadquaterIn rdf:type owl:ObjectProperty .


###  https://w3id.org/i40/sto/hasICS
:hasICS rdf:type owl:ObjectProperty ;
        rdfs:domain :Publication ;
        rdfs:range :InternationalClassificationForStandards ;
        rdfs:comment "Serves as a catalogue of International Classification For Standards"@en ;
        rdfs:label "has ICS"@en .


###  https://w3id.org/i40/sto/hasPublisher
:hasPublisher rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf foaf:maker ;
              rdfs:domain :Publication ;
              rdfs:range :StandardOrganization .


###  https://w3id.org/i40/sto/hasTargetConcern
:hasTargetConcern rdf:type owl:ObjectProperty ;
                  rdfs:domain :Publication ;
                  rdfs:range :Concern ;
                  rdfs:comment "The relation declaring that a standard is relevant or targets a certain concern."@en ;
                  rdfs:label "has Target Concern"@en .


###  https://w3id.org/i40/sto/hasTechnicalCommittee
:hasTechnicalCommittee rdf:type owl:ObjectProperty ;
                       rdfs:domain :Publication ;
                       rdfs:range :TechnicalCommittee ;
                       rdfs:comment "Specifies the Technical Committee or a Sub-Committee that is responsible for creation and maintenance of a standard"@en ;
                       rdfs:label "has Technical Committee"@en .


###  https://w3id.org/i40/sto/identifies
:identifies rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf dul:associatedWith ;
            rdfs:comment "ISO 42010 relation between an architecture description on the on hand and stakeholders and concerns on the other." ;
            rdfs:label "identifies" .


###  https://w3id.org/i40/sto/isDescribedin
:isDescribedin rdf:type owl:ObjectProperty ;
               rdfs:domain :StandardClassification ;
               rdfs:range :StandardizationFramework ;
               rdfs:comment "Links the Standarization Frameworks with the Classification of Standards"@en ;
               rdfs:label "is Described in"@en .


###  https://w3id.org/i40/sto/isImplementationOf
:isImplementationOf rdf:type owl:ObjectProperty .


###  https://w3id.org/i40/sto/relatedTo
:relatedTo rdf:type owl:ObjectProperty ;
           owl:inverseOf :relatedTo ;
           rdf:type owl:SymmetricProperty ,
                    owl:TransitiveProperty .


###  https://w3id.org/i40/sto/useStructureOf
:useStructureOf rdf:type owl:ObjectProperty .


###  https://w3id.org/i40/sto/uses
:uses rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://creativecommons.org/ns#attributionName
cc:attributionName rdf:type owl:DatatypeProperty .


###  http://www.ontology-of-units-of-measure.org/resource/om-2/hasNumericalValue
om2:hasNumericalValue rdf:type owl:DatatypeProperty ;
                      rdfs:label "has numerical value"@en .


###  https://w3id.org/i40/sto/abbreviation
:abbreviation rdf:type owl:DatatypeProperty ;
              rdfs:domain owl:Thing ;
              rdfs:range rdf:PlainLiteral ;
              rdfs:comment "Abbreviated  name of the thing."@en ;
              rdfs:label "has Abbreviated Name"@en .


###  https://w3id.org/i40/sto/domainName
:domainName rdf:type owl:DatatypeProperty ;
            rdfs:domain :Domain ;
            rdfs:range rdf:PlainLiteral ;
            rdfs:comment "Points the Standard with the name of the Industrial Domain"@en ;
            rdfs:label "has Domain Name"@en .


###  https://w3id.org/i40/sto/formationDate
:formationDate rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :StandardOrganization ;
               rdfs:range xsd:date ;
               rdfs:comment "Formation date of the Organization"@en ;
               rdfs:label "has Formation Date"@en .


###  https://w3id.org/i40/sto/hasEdition
:hasEdition rdf:type owl:DatatypeProperty ;
            rdfs:domain foaf:Document ;
            rdfs:range xsd:float ;
            rdfs:comment "Indicates the current Edition/Version number of the document."@en ;
            rdfs:label "has Edition"@en .


###  https://w3id.org/i40/sto/hasImplementationFormat
:hasImplementationFormat rdf:type owl:DatatypeProperty ;
                         rdfs:domain :Publication ;
                         rdfs:range rdfs:Literal ;
                         rdfs:comment "Describes the name of the implementation or serialization format of the Publication, e.g., XML, Document, binary, batch"@en ;
                         rdfs:label "has Implementation Format"@en .


###  https://w3id.org/i40/sto/hasPages
:hasPages rdf:type owl:DatatypeProperty ;
          rdfs:domain foaf:Document ;
          rdfs:range xsd:integer ;
          rdfs:comment "Indicates the number of pages in the document."@en ;
          rdfs:label "has Pages"@en .


###  https://w3id.org/i40/sto/hasPublicationDate
:hasPublicationDate rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Publication ;
                    rdfs:range xsd:date ;
                    rdfs:comment "Publication date of the Publication"@en ;
                    rdfs:label "has Publication Date"@en .


###  https://w3id.org/i40/sto/hasStabilityDate
:hasStabilityDate rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Publication ;
                  rdfs:range xsd:gYear ;
                  rdfs:comment "Stability date of a publication. Publications are valid for a certain period, i.e., the Stability Period. This property defines the end of the stability period."@en ;
                  rdfs:label "has Stability Date"@en ;
                  rdfs:seeAlso <https://webstore.iec.ch/webstore/webstore.nsf/xpFAQ.xsp?Open&id=ADMN-7NALM5> ;
                  skos:note "The data specified is not actually a Date but an Year" .


###  https://w3id.org/i40/sto/hasStatus
:hasStatus rdf:type owl:DatatypeProperty ;
           rdfs:domain :Publication ;
           rdfs:range rdfs:Literal ;
           rdfs:comment "Publication status of the Standard, e.g., Valid, Revised, Published, under preparation, Released"@en ;
           rdfs:label "has Status"@en .


###  https://w3id.org/i40/sto/orgName
:orgName rdf:type owl:DatatypeProperty ;
         rdfs:domain :StandardOrganization ;
         rdfs:range rdf:PlainLiteral ;
         rdfs:comment "Name of the Organization"@en ;
         rdfs:label "Org Name"@en .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology#Country
dbo:Country rdf:type owl:Class .


###  http://dbpedia.org/ontology#Place
dbo:Place rdf:type owl:Class .


###  http://example.org/iira#CrosscuttingFunction
iira:CrosscuttingFunction rdf:type owl:Class ;
                          rdfs:subClassOf :StandardClassification ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty dul:isPartOf ;
                                            owl:hasValue iira:FunctionalViewpoint
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :isDescribedin ;
                                            owl:hasValue :IIRA
                                          ] .


###  http://example.org/iira#FunctionalDomain
iira:FunctionalDomain rdf:type owl:Class ;
                      rdfs:subClassOf :StandardClassification ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty dul:isPartOf ;
                                        owl:hasValue iira:FunctionalViewpoint
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :isDescribedin ;
                                        owl:hasValue :IIRA
                                      ] ;
                      rdfs:label "IIRA Functional Domain"@en .


###  http://example.org/iira#SystemCharacteristics
iira:SystemCharacteristics rdf:type owl:Class ;
                           rdfs:subClassOf :StandardClassification ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty dul:isPartOf ;
                                             owl:hasValue iira:FunctionalViewpoint
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :isDescribedin ;
                                             owl:hasValue :IIRA
