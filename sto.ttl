###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:ObjectProperty ;
                owl:equivalentProperty foaf:maker ;
                rdfs:comment "An entity primarily responsible for making the resource."@en ;
                dcterms:hasVersion <http://dublincore.org/usage/terms/history/#creatorT-002> ;
                rdfs:isDefinedBy dcterms: ;
                dcterms:modified "2010-10-11"^^xsd:date ;
                rdfs:label "has Creator"@en ;
                dcterms:issued "2008-01-14"^^xsd:date ;
                dcterms:description "Examples of a Creator include a person, an organization, or a service."@en .


###  https://w3id.org/i40/sto#isImplementationOf
sto:isImplementationOf rdf:type owl:ObjectProperty ;
                       dcterms:creator sto:irlan .


###  http://purl.org/dc/terms/language
dcterms:language rdf:type owl:ObjectProperty .


###  http://www.ontology-of-units-of-measure.org/resource/om-2/hasUnit
om2:hasUnit rdf:type owl:ObjectProperty ;
            rdfs:range om2:Unit ;
            rdfs:label "has unit"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith
dul:associatedWith rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isComponentOf
dul:isComponentOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf sto:relatedTo ;
                  rdfs:domain sto:Standard ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( sto:Standard
                                             sto:StandardPart
                                           )
                             ] ;
                  rdfs:comment "Represents Standards that are component of other Standards. This relation is not transitive"@en ;
                  rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
                  rdfs:label "is Component Of"@en .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isMemberOf
dul:isMemberOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf dul:associatedWith ;
               rdfs:range dul:Collection ;
               rdfs:comment "A relation between collections and entities, e.g. 'the Night Watch by Rembrandt is in the Rijksmuseum collection'; 'Davide is member of the Pen Club', 'Igor is one the subjects chosen for the experiment'."@en ;
               rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
               rdfs:label "is member of"@en ,
                          "è membro di"@it .


###  http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isPartOf
dul:isPartOf rdf:type owl:ObjectProperty ;
             rdfs:label "isPartOf"@en .



###  https://w3id.org/i40/sto#isPartOf
sto:isPartOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf dul:isPartOf ;
             rdfs:domain sto:Publication ;
             rdfs:range sto:Publication ;
			 rdfs:comment "specifies that one publication is a part or a section of another publication. Mostly used to indicate that a sto:Standard is a part of standard familiy or group of standards."@en ;
             rdfs:label "is part of"@en .



###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf sto:relatedTo ;
                    rdfs:comment "Standard A has influences in Standard B. For example, IEC_61499 hasInfluenceIn IEC_61804-2(EDDL)"@en ;
                    rdfs:isDefinedBy <https://www.w3.org/TR/prov-o/#wasGeneratedBy> ;
                    rdfs:label "was Generated By"@en .


###  http://xmlns.com/foaf/0.1/maker
foaf:maker rdf:type owl:ObjectProperty .


###  https://w3id.org/i40/sto#alignsWith
sto:alignsWith rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf dul:associatedWith ;
               rdf:type owl:SymmetricProperty ;
               rdfs:domain sto:StandardClassification ;
               rdfs:range sto:StandardClassification ;
               rdfs:comment "indicates that some Standard Classification entity has a similar scope as another. This property most probably is symmetric but not transitive."@en ;
               rdfs:label "alignes With"@en .


###  https://w3id.org/i40/sto#belongsTo
sto:belongsTo rdf:type owl:ObjectProperty ;
              rdfs:domain sto:StandardOrganization ;
              rdfs:range dbo:Country ;
              dcterms:creator sto:irlan;
              rdfs:label "belongs To"@en ;
              rdfs:comment "A Standard Organization that belongs to the country"@en ;
              .



###  https://w3id.org/i40/sto#hasAvailableLanguage
sto:hasAvailableLanguage rdf:type owl:ObjectProperty ;
                         rdfs:range lvont:Language;
                         rdfs:domain foaf:Document ;
                         rdfs:label "hasAvailableLanguage"@en ;
                         rdfs:comment "Points to Languages in which the standard document is available, e.g., English, French, etc. Defined as subProperty of dcterms:language"@en ,
                                      "Languages in which the document of the Standard is available, e.g., English, French, Spanish"@en .


###  https://w3id.org/i40/sto#hasClassification
sto:hasClassification rdf:type owl:ObjectProperty ;
                      rdfs:domain sto:Publication ;
                      rdfs:range sto:StandardClassification ;
                      dcterms:creator sto:irlan .


###  https://w3id.org/i40/sto#hasDeveloper
sto:hasDeveloper rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf foaf:maker ;
                 rdfs:label "has Developer"@en;
                 rdfs:comment "Links the Standard Organization that developed the Publication with the specific type of publication, e.g., Standard, Technical Report"@en;
                 rdfs:domain sto:Publication ;
                 rdfs:range sto:StandardOrganization ;
                 dcterms:creator sto:irlan ;                 
                 .


###  https://w3id.org/i40/sto#hasDimension
sto:hasDimension rdf:type owl:ObjectProperty ;
                 rdfs:domain sto:StandardizationFramework ;
                 rdfs:range sto:Dimension ;
                 rdfs:comment "Links the Standardization Frameworks with the Dimensions."@en ;
                 rdfs:label "has Dimension"@en .


###  https://w3id.org/i40/sto#hasDimensionLayer
sto:hasDimensionLayer rdf:type owl:ObjectProperty ;
                      rdfs:domain sto:Dimension ;
                      rdfs:range sto:Layer ;
                      rdfs:comment "Links the Dimensions with Layers."@en ;
                      rdfs:label "has Dimension Layer"@en .


###  https://w3id.org/i40/sto#hasDomain
sto:hasDomain rdf:type owl:ObjectProperty ;
              rdfs:domain sto:Publication ;
              rdfs:range sto:Domain ;
              rdfs:comment "Describes the domain, in which the Publication is used, particularly, standards."@en ;
              rdfs:label "has Domain"@en .


###  https://w3id.org/i40/sto#hasFileSize
sto:hasFileSize rdf:type owl:ObjectProperty,
                         owl:FunctionalProperty ;
                rdfs:domain foaf:Document ;
                rdfs:range om2:Measure .


###  https://w3id.org/i40/sto#hasHeadquaterIn
sto:hasHeadquaterIn rdf:type owl:ObjectProperty ;
                    rdfs:domain sto:StandardOrganization ;
                    rdfs:range dbo:Place .


###  https://w3id.org/i40/sto#hasICS
sto:hasICS rdf:type owl:ObjectProperty;
           rdfs:domain sto:Publication ;
           rdfs:range sto:InternationalClassificationForStandards ;
           rdfs:comment "Serves as a catalogue of International Classification For Standards"@en ;
           rdfs:label "has ICS"@en .


###  https://w3id.org/i40/sto#hasLayer
sto:hasLayer rdf:type owl:ObjectProperty ;
             rdfs:domain sto:StandardizationFramework ;
             rdfs:range sto:Layer ;
             rdfs:comment "Links the Standardization Frameworks with the Layers "@en ;
             rdfs:label "has Layer"@en .


###  https://w3id.org/i40/sto#hasPreviousVersion
sto:hasPreviousVersion rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf sto:relatedTo ;
                       rdfs:comment "Indicates the list of Standards that are previous versions of a Standard. For example,IEC 60038:2009 has IEC 60038:1983+AMD1:1994+AMD2:1997,IEC 60038:1983/AMD2:1997,IEC 60038:1983/AMD1:1994,IEC 60038:1983,IEC 60038:1975/AMD1:1977,IEC 60038:1975,IEC 60038:1967,IEC 60038:1954,IEC 60038:1938,IEC 60038:1927 as previous versions."@en ;
                       rdfs:label "has Previous Version"@en .


###  https://w3id.org/i40/sto#hasPublisher
sto:hasPublisher rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf foaf:maker ;
                 rdfs:domain sto:Publication ;
                 rdfs:range sto:StandardOrganization .


###  https://w3id.org/i40/sto#hasTag
sto:hasTag rdf:type owl:ObjectProperty ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( muto:Tag
                                      rdfs:Literal
                                    )
                      ] .


###  https://w3id.org/i40/sto#hasTechnicalCommittee
sto:hasTechnicalCommittee rdf:type owl:ObjectProperty ;
                          rdfs:domain sto:Publication ;
                          rdfs:range sto:TechnicalCommittee ;
                          rdfs:comment "Specifies the Technical Committee or a Sub-Committee that is responsible for creation and maintenance of a standard"@en ;
                          rdfs:label "has Technical Committee"@en .


###  https://w3id.org/i40/sto#identifies
sto:identifies rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf dul:associatedWith ;
               rdfs:comment "ISO 42010 relation between an architecture description on the on hand and stakeholders and concerns on the other."@en ;
               rdfs:label "identifies"@en .


###  https://w3id.org/i40/sto#isDescribedin
sto:isDescribedin rdf:type owl:ObjectProperty ;
                  rdfs:domain sto:StandardClassification ;
                  rdfs:range sto:StandardizationFramework ;
                  rdfs:comment "Links the Standarization Frameworks with the Classification of Standards"@en ;
                  rdfs:label "is Described in"@en .


###  https://w3id.org/i40/sto#isImplementationOf
sto:isImplementationOf rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf sto:relatedTo .


###  https://w3id.org/i40/sto#isPartOf
sto:isPartOf rdf:type owl:ObjectProperty ;
             rdfs:domain sto:Publication ;
             rdfs:range sto:Publication ;
             rdfs:comment "specifies that one publication is a part or a section of another publication. Mostly used to indicate that a sto:Standard is a part of standard familiy or group of standards."@en ;
