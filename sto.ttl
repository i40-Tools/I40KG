  rdfs:seeAlso "https://ieeexplore.ieee.org/document/8463303"@en ;
  rdfs:seeAlso "https://ieeexplore.ieee.org/document/8474855"@en ;
  skos:note "Publications referred: '<https://www.openfogconsortium.org/wp-content/uploads/OpenFog_Reference_Architecture_2_09_17-FINAL.pdf> OPFRA001.020817' '4.6 Reliability, Availability, and Serviceability (RAS)Pillar' 'Page 29' " ;
