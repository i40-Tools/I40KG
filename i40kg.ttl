sto:SysML rdf:type owl:NamedIndividual ,
                   foaf:Document ,
                   sto:Publication ,
                   sto:Standard ;
          dcterms:creator sto:OMG ;
          foaf:maker sto:OMG ;
          sto:hasDeveloper sto:OMG ;
          sto:hasPublisher sto:OMG ;
          sto:relatedTo sto:DIN_SPEC_16592 ,
                        sto:IEC_61131 ,
                        sto:IEC_61360 ,
                        sto:IEC_61499 ,
                        sto:IEC_61804 ,
                        sto:IEC_61850 ,
                        sto:IEC_61987_X ,
